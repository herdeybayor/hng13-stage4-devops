{
  "subnet": "10.0.1.0/24",
  "description": "Web server policy - allows HTTP and HTTPS",
  "ingress": [
    {
      "port": 80,
      "protocol": "tcp",
      "action": "allow",
      "source": "0.0.0.0/0"
    },
    {
      "port": 443,
      "protocol": "tcp",
      "action": "allow",
      "source": "0.0.0.0/0"
    },
    {
      "port": 8080,
      "protocol": "tcp",
      "action": "allow",
      "source": "0.0.0.0/0"
    }
  ],
  "egress": [
    {
      "port": "*",
      "protocol": "tcp",
      "action": "allow",
      "destination": "0.0.0.0/0"
    }
  ]
}

